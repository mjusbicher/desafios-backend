<head>
   <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
    <h1 class="text-center">Bienvenido {{name}}</h1>
<table class="table table-bordered table-striped text-center mt-4" >
    {{#each object}}
 <thead>
    <tr>
      <th scope="col">Nombre: {{this.name}}</th>
      <th scope="col">Precio: {{this.precio}}</th>
      <th scope="col">Color: {{this.color}}</th>
      <th scope="col"> Imagen: <img scope="row" src="http://localhost:8080/img/{{this.image}}"></th>
    </tr>
 </thead>
  <tbody>
    {{/each}}
</table>

 <div class="mx-auto text-center">
    <a class="btn btn-dark mx-auto text-center my-5" href="/newObject">Ir a crear productos</a>
 </div>      